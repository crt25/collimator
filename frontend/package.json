{
  "name": "collimator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "echo 'Please check https://github.com/getsentry/sentry-javascript/issues/8105 and upgrade to turbopack if possible' && cross-env NODE_OPTIONS=--preserve-symlinks next dev -p 3000",
    "dev:turbopack": "cross-env NODE_OPTIONS=--preserve-symlinks next dev --turbopack -p 3000",
    "dev:coverage": "yarn test:cov:enable && BABEL_ENV=coverage next dev && yarn test:cov:disable",
    "build": "yarn chakra:update && cross-env NODE_OPTIONS=--preserve-symlinks next build",
    "build:coverage": "yarn chakra:update && yarn test:cov:enable && BABEL_ENV=coverage next build && yarn test:cov:disable",
    "start": "cross-env NODE_OPTIONS=--preserve-symlinks next start -p 3000",
    "lint": "next lint",
    "update:api": "shx rm -rf src/api/collimator/generated && orval",
    "i18n:extract": "formatjs extract 'src/{pages,components,i18n,sections}/**/*.{js,ts,tsx}' --format crowdin --out-file content/locales/en.json",
    "i18n:compile": "formatjs compile-folder --ast --format crowdin content/locales content/compiled-locales",
    "i18n:fill": "node ./scripts/fill-locales.mjs",
    "i18n:update": "yarn i18n:extract && yarn i18n:fill && yarn i18n:compile",
    "chakra:update": "npx @chakra-ui/cli typegen ./src/components/ui/Theme.ts",
    "test": "yarn test:jest && yarn test:playwright",
    "test:cov:enable": "shx cp .babelrc.disabled .babelrc",
    "test:cov:disable": "shx rm .babelrc",
    "test:cov": "yarn test:cov:enable && yarn test:jest:cov && yarn test:playwright && yarn test:cov:disable",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:cov": "jest --coverage",
    "test:jest:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:pre": "shx rm -rf coverage-ct/ .nyc_output/",
    "test:ct": "yarn test:pre && playwright test -c playwright-ct.config.ts && nyc report --report-dir coverage-ct",
    "test:ct:debug": "yarn test:pre && playwright test -c playwright-ct.config.ts --debug && nyc report --report-dir coverage-ct",
    "test:playwright": "yarn test:ct",
    "test:storybook": "test-storybook --coverage --coverageDirectory coverage-storybook",
    "storybook": "cross-env NODE_OPTIONS=--preserve-symlinks storybook dev -p 6006",
    "storybook:await": "bash -c \"until curl --output /dev/null --silent --head --fail http://localhost:6006; do printf '.'; sleep 3; done\"",
    "storybook:build": "storybook build",
    "storybook:update": "cd storybook-submodule && git checkout github-pages/storybook && git fetch && git reset --hard origin/github-pages/storybook && git rm -r . && shx cp -r ../storybook-static/. ./ && git add -A && git commit && git push",
    "next:upgrade": "npx @next/codemod@canary upgrade latest"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^3.28.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@hookform/resolvers": "^4.1.3",
    "@sentry/nextjs": "^9.13.0",
    "@tanstack/react-table": "^8.21.3",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.8",
    "chartjs-plugin-annotation": "^3.1.0",
    "edit-distance": "^1.0.5",
    "iframe-rpc-react": "portal:../libraries/iframe-rpc-react",
    "jqgram": "^0.2.5",
    "next": "15.3.1",
    "next-themes": "^0.4.6",
    "openid-client": "^6.3.4",
    "patternomaly": "^1.3.2",
    "playwright-test-coverage": "^1.2.12",
    "primereact": "^10.9.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "react-intl": "^7.1.10",
    "shx": "^0.4.0",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.3",
    "ts-pattern": "^5.7.0",
    "unique-names-generator": "^4.7.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@faker-js/faker": "^9.5.0",
    "@formatjs/cli": "^6.6.1",
    "@playwright/experimental-ct-react": "^1.50.1",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-essentials": "^8.6.2",
    "@storybook/addon-interactions": "^8.6.2",
    "@storybook/addon-links": "^8.6.2",
    "@storybook/blocks": "^8.6.2",
    "@storybook/nextjs": "^8.6.2",
    "@storybook/preview-api": "^8.6.2",
    "@storybook/react": "^8.6.2",
    "@storybook/test": "^8.6.2",
    "@storybook/test-runner": "^0.21.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bootstrap": "^5.2.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "babel-plugin-formatjs": "^10.5.35",
    "babel-plugin-istanbul": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-extended": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-storybook": "^0.11.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "msw": "^2.7.3",
    "nyc": "^17.1.0",
    "orval": "^7.6.0",
    "prettier": "^3.5.2",
    "sass": "^1.85.1",
    "storybook": "^8.6.2",
    "storybook-addon-mock": "^5.0.0",
    "storybook-react-intl": "^3.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite-plugin-istanbul": "^7.0.0",
    "wait-for-expect": "^3.0.2"
  },
  "packageManager": "yarn@4.5.0",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "resolutions": {
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2"
  }
}
