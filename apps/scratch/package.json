    "redux": "^3.0.0",
    "redux-throttle": "^0.1.1",
    "scratch-audio": "^1.0.258",
    "scratch-blocks": "patch:scratch-blocks@npm%3A1.1.206#~/.yarn/patches/scratch-blocks-npm-1.1.206-87dc9f1fd2.patch",
    "scratch-l10n": "^3.18.313",
    "scratch-paint": "^2.2.408",
    "scratch-render": "^1.1.39",
    "scratch-render-fonts": "^1.0.102",
    "scratch-storage": "^4.0.23",
    "scratch-svg-renderer": "^2.4.15",
    "scratch-vm": "patch:scratch-vm@npm%3A4.6.29#~/.yarn/patches/scratch-vm-npm-4.6.29-f3a8bb62d5.patch",
    "shx": "^0.3.4",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "to-style": "^1.3.3",
    "typescript": "^5.7.2",
    "url-loader": "^4.1.1",
    "wav-encoder": "^1.3.0",
    "web-vitals": "^4.2.4",
    "webpack": "^5.97.1",
    "xhr": "^2.6.0"
  },
  "resolutions": {
    "@svgr/webpack": "6.5.1",
    "@types/react": "16.14.61",
    "babel-preset-react-app@npm:^10.0.1": "patch:babel-preset-react-app@npm%3A10.0.1#~/.yarn/patches/babel-preset-react-app-npm-10.0.1-a9a19b630a.patch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)sx?"
    ],
    "coveragePathIgnorePatterns": [
      "src/scratch",
      "__tests__",
      "\\.d\\.ts$",
      "\\.spec\\.(t|j)sx?$"
    ],
    "moduleNameMapper": {
      "@scratch-submodule/(.*)": "<rootDir>/src/scratch/$1",
      "!ify-loader!(.*)": "<rootDir>/node_modules/$1",
      "!arraybuffer-loader!(.*)": "<rootDir>/src/tests/binary.ts",
      "!raw-loader!(.*)": "<rootDir>/src/tests/binary.ts",
      "!base64-loader!(.*)": "<rootDir>/src/tests/binary.ts"
    }
  },
  "nyc": {
    "all": true,
    "parserPlugins": [
      "typescript",
      "jsx",
      "asyncGenerators",
      "bigInt",
      "classProperties",
      "classPrivateProperties",
      "classPrivateMethods",
      "dynamicImport",
      "importMeta",
      "numericSeparator",
      "objectRestSpread",
      "optionalCatchBinding",
      "topLevelAwait"
    ],
    "extensions": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.config.js",
      "**/*.d.ts",
      "**/node_modules/**/*",
      "**/__mocks__/**/*",
      "**/__tests__/**/*",
      "src/scratch/**/*",
      "src/setupTests.ts",
      "src/tests/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "packageManager": "yarn@4.5.0",
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "^29.5.14",
    "@types/keymirror": "^0",
    "@types/lodash.bindall": "^4.4.9",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.defaultsdeep": "^4",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^22.9.0",
    "@types/omggif": "^1",
    "@types/papaparse": "^5",
    "@types/prop-types": "^15",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-intl": "^2.2.2",
    "@types/react-modal": "^3",
    "@types/react-popover": "^0",
    "@types/react-redux": "^7.1.34",
    "@types/react-responsive": "^8.0.8",
    "@types/react-tabs": "^2.3.0",
    "@types/react-virtualized": "^9",
    "@types/wav-encoder": "^1",
    "archiver": "^7.0.1",
    "nyc": "^17.1.0"
  }
}
