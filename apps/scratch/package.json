{
  "name": "app-scratch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--preserve-symlinks next dev --webpack --port 3101",
    "dev:coverage": "yarn test:cov:enable && cross-env NODE_OPTIONS=--preserve-symlinks BABEL_ENV=coverage next dev --webpack --port 3101 && yarn test:cov:disable",
    "build": "cross-env NODE_OPTIONS=--preserve-symlinks next build --webpack",
    "start": "cross-env NODE_OPTIONS=--preserve-symlinks next start --port 3101",
    "pretest": "shx rm -rf coverage/ .nyc_output/ && yarn test:cov:enable",
    "test": "yarn pretest && playwright test && yarn posttest",
    "test:cov:enable": "shx cp .babelrc.disabled .babelrc",
    "test:cov:disable": "shx rm .babelrc",
    "test:local": "yarn pretest && SERVER_ALREADY_RUNS=true playwright test && yarn posttest",
    "test:debug": "SERVER_ALREADY_RUNS=true playwright test --debug --project='iPad Mini landscape'",
    "posttest": "nyc report && yarn test:cov:disable",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "i18n:extract": "formatjs extract 'src/{components,sections,extensions,hooks}/**/*.{js,ts,tsx}' --format crowdin --out-file src/content/locales/en.json",
    "i18n:compile": "formatjs compile-folder --ast --format crowdin src/content/locales src/content/compiled-locales",
    "i18n:fill": "node ./scripts/fill-locales.mjs",
    "i18n:update": "yarn i18n:extract && yarn i18n:fill && yarn i18n:compile",
    "postinstall": "node scripts/postinstall.mjs && yarn patch:scratch-editor",
    "patch:scratch-editor": "cd ./src/scratch-editor && git reset --hard && git apply ../../scratch-editor.patch",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org hep-vaud --project crt-scratch ./build && sentry-cli sourcemaps upload --org hep-vaud --project crt-scratch ./build"
  },
  "homepage": "/scratch",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/compat": "^2.0.0",
    "@microbit/microbit-universal-hex": "^0.2.2",
    "@playwright/test": "^1.51.1",
    "@radix-ui/react-context-menu": "^2.2.5",
    "@scratch/scratch-render": "11.6.0-react-18",
    "@scratch/scratch-vm": "patch:@scratch/scratch-vm@npm%3A11.6.0-react-18#~/.yarn/patches/@scratch-scratch-vm-npm-11.6.0-react-18-c04086e700.patch",
    "@sentry/cli": "^2.43.0",
    "@sentry/nextjs": "^10.31.0",
    "@sentry/react": "^9.11.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@turbowarp/types": "^0.0.14",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "arraybuffer-loader": "^1.0.8",
    "autoprefixer": "^10.4.21",
    "babel-plugin-istanbul": "^7.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "balance-text": "^3.3.1",
    "bowser": "^2.11.0",
    "buffer": "^6.0.3",
    "cat-blocks": "npm:scratch-blocks@0.1.0-prerelease.20220318143026",
    "computed-style-to-inline-style": "^4.0.0",
    "cookie": "^0.6.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "5.2.7",
    "dapjs": "^2.3.0",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-scratch": "9.0.9",
    "eslint-plugin-prettier": "^5.2.6",
    "get-float-time-domain-data": "^0.1.0",
    "get-user-media-promise": "1.1.4",
    "globals": "^16.5.0",
    "iframe-rpc-react": "portal:../../libraries/iframe-rpc-react",
    "immutable": "^5.1.1",
    "js-base64": "^3.7.7",
    "jszip": "^3.10.1",
    "keymirror": "^0.1.1",
    "lodash.defaultsdeep": "^4.6.1",
    "lru-cache": "^11.2.4",
    "next": "^16.0.10",
    "omggif": "^1.0.10",
    "papaparse": "^5.5.2",
    "playwright": "^1.51.1",
    "playwright-test-coverage": "^1.2.12",
    "postcss": "^8.5.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.5.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-draggable": "^4.4.6",
    "react-hot-toast": "^2.5.2",
    "react-intl": "6.8.9",
    "react-intl-redux": "^2.4.1",
    "react-modal": "^3.16.3",
    "react-popover": "^0.5.10",
    "react-redux": "^8.0.0",
    "react-responsive": "9.0.0",
    "react-style-proptype": "^3.2.2",
    "react-tabs": "^2.3.0",
    "react-tooltip": "^4.0.0",
    "react-virtualized": "^9.22.6",
    "react-visibility-sensor": "5.1.1",
    "redux": "^4.0.0",
    "redux-throttle": "^0.1.1",
    "scratch-audio": "2.0.215",
    "scratch-blocks": "patch:scratch-blocks@npm%3A1.1.206#~/.yarn/patches/scratch-blocks-npm-1.1.206-87dc9f1fd2.patch",
    "scratch-l10n": "6.0.30",
    "scratch-paint": "4.0.3",
    "scratch-render-fonts": "1.0.220",
    "scratch-storage": "patch:scratch-storage@npm%3A4.0.210#~/.yarn/patches/scratch-storage-npm-4.0.210-b74f040f66.patch",
    "scratch-svg-renderer": "^2.4.15",
    "shx": "^0.4.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "4.0.0",
    "to-style": "^1.3.3",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "wav-encoder": "^1.3.0",
    "web-vitals": "^4.2.4",
    "xhr": "^2.6.0"
  },
  "resolutions": {
    "@svgr/webpack": "6.5.1",
    "@types/react": "18.3.1",
    "babel-preset-react-app@npm:^10.0.1": "patch:babel-preset-react-app@npm%3A10.0.1#~/.yarn/patches/babel-preset-react-app-npm-10.0.1-a9a19b630a.patch",
    "scratch-storage@npm:4.0.210": "patch:scratch-storage@npm%3A4.0.210#~/.yarn/patches/scratch-storage-npm-4.0.210-b74f040f66.patch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)sx?"
    ],
    "coveragePathIgnorePatterns": [
      "src/scratch",
      "__tests__",
      "\\.d\\.ts$",
      "\\.spec\\.(t|j)sx?$"
    ],
    "moduleNameMapper": {
      "@scratch-submodule/(.*)": "<rootDir>/src/scratch-editor/$1",
      "!ify-loader!(.*)": "<rootDir>/node_modules/$1",
      "!arraybuffer-loader!(.*)": "<rootDir>/src/tests/binary.ts",
      "!raw-loader!(.*)": "<rootDir>/src/tests/binary.ts",
      "!base64-loader!(.*)": "<rootDir>/src/tests/binary.ts"
    }
  },
  "nyc": {
    "all": true,
    "parserPlugins": [
      "typescript",
      "jsx",
      "asyncGenerators",
      "bigInt",
      "classProperties",
      "classPrivateProperties",
      "classPrivateMethods",
      "dynamicImport",
      "importMeta",
      "numericSeparator",
      "objectRestSpread",
      "optionalCatchBinding",
      "topLevelAwait"
    ],
    "extensions": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.config.js",
      "**/*.d.ts",
      "**/node_modules/**/*",
      "**/__mocks__/**/*",
      "**/__tests__/**/*",
      "src/scratch/**/*",
      "src/setupTests.ts",
      "src/tests/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "packageManager": "yarn@4.5.0",
  "devDependencies": {
    "@formatjs/cli": "^6.6.3",
    "@types/archiver": "^6.0.3",
    "@types/jest": "^29.5.14",
    "@types/keymirror": "^0",
    "@types/lodash.bindall": "^4.4.9",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.defaultsdeep": "^4",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^22.14.0",
    "@types/omggif": "^1",
    "@types/papaparse": "^5",
    "@types/postcss-flexbugs-fixes": "^5",
    "@types/prop-types": "^15",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/react-modal": "^3",
    "@types/react-popover": "^0",
    "@types/react-responsive": "^8.0.8",
    "@types/react-tabs": "^2.3.0",
    "@types/react-virtualized": "^9",
    "@types/wav-encoder": "^1",
    "@types/webpack": "^5.28.5",
    "archiver": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-import": "^2.32.0",
    "nyc": "^17.1.0"
  }
}
